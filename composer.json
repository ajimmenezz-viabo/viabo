      "Viabo\\Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/framework-bundle": "^6.2",
    "symfony/dotenv": "^6.2",
    "symfony/yaml": "^6.2",
    "symfony/twig-bundle": "^6.2",
    "symfony/mailer": "^6.2",
