<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\tickets\ticket\domain\TicketFile" table="t_tickets_files">
        <embedded name="id" class="TicketFileId" use-column-prefix="false"/>
        <embedded name="ticketId" class="TicketFileReferenceId" use-column-prefix="false"/>
        <embedded name="name" class="TicketFileName" use-column-prefix="false"/>
        <embedded name="storagePath" class="TicketFileStoragePath" use-column-prefix="false"/>
        <embedded name="createDate" class="TicketFileCreateDate" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>