<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\business\commission\domain\Commission"  table="t_business_commerces_commissions">
        <embedded name="id" class="CommissionId" use-column-prefix="false"/>
        <id name="commerceId" type="commerce_id" column="commerceId"/>
        <embedded name="speiInCarnet" class="CommissionSpeiInCarnet" use-column-prefix="false"/>
        <embedded name="speiInMasterCard" class="CommissionSpeiInMasterCard" use-column-prefix="false"/>
        <embedded name="speiOutCarnet" class="CommissionSpeiOutCarnet" use-column-prefix="false"/>
        <embedded name="speiOutMasterCard" class="CommissionSpeiOutMasterCard" use-column-prefix="false"/>
        <embedded name="pay" class="CommissionPay" use-column-prefix="false"/>
        <embedded name="updateByUser" class="CommissionUpdateByUser" use-column-prefix="false"/>
        <embedded name="updateDate" class="CommissionUpdateDate" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>