<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\management\commercePay\domain\CommercePay" table="t_management_commerces_pay">
        <embedded name="id" class="CommercePayId" use-column-prefix="false"/>
        <embedded name="userId" class="CommercePayUserId" use-column-prefix="false"/>
        <embedded name="commerceId" class="CommercePayCommerceId" use-column-prefix="false"/>
        <embedded name="terminalId" class="CommercePayTerminalId" use-column-prefix="false"/>
        <embedded name="fullName" class="CommercePayFullName" use-column-prefix="false"/>
        <embedded name="email" class="CommercePayEmail" use-column-prefix="false"/>
        <embedded name="phone" class="CommercePayPhone" use-column-prefix="false"/>
        <embedded name="description" class="CommercePayDescription" use-column-prefix="false"/>
        <embedded name="amount" class="CommercePayAmount" use-column-prefix="false"/>
        <embedded name="urlCode" class="CommercePayUrlCode" use-column-prefix="false"/>
        <embedded name="registerDate" class="CommercePayRegisterDate" use-column-prefix="false"/>
        <embedded name="active" class="CommercePayActive" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>
