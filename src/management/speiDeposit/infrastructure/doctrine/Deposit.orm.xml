<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\management\speiDeposit\domain\Deposit" table="t_management_spei_deposits">
        <embedded name="id" class="DepositId" use-column-prefix="false"/>
        <embedded name="reference" class="DepositReference" use-column-prefix="false"/>
        <embedded name="apiKey" class="DepositAPIKey" use-column-prefix="false"/>
        <embedded name="data" class="DepositData" use-column-prefix="false"/>
        <embedded name="registerDate" class="DepositRegisterDate" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>