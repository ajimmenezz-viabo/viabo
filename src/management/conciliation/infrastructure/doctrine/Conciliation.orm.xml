<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\management\conciliation\domain\Conciliation" table="t_management_conciliation">
        <embedded name="id" class="ConciliationId" use-column-prefix="false"/>
        <embedded name="referenceNumber" class="ConciliationReferenceNumber" use-column-prefix="false"/>
        <field name="cardId" type="card_id" column="CardId"/>
        <embedded name="amount" class="ConciliationAmount" use-column-prefix="false"/>
        <embedded name="spei" class="ConciliationSpei" use-column-prefix="false"/>
        <embedded name="referencePayCash" class="ConciliationReferencePayCash" use-column-prefix="false"/>
        <embedded name="movementNumber" class="ConciliationMovementNumber" use-column-prefix="false"/>
        <embedded name="emails" class="ConciliationEmails" use-column-prefix="false"/>
        <embedded name="registerDate" class="ConciliationRegisterDate" use-column-prefix="false"/>
        <embedded name="active" class="ConciliationActive" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>