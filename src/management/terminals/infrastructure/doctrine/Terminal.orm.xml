        <embedded name="commerceId" class="TerminalCommerceId" use-column-prefix="false"/>
        <embedded name="serial" class="TerminalSerial" use-column-prefix="false"/>
        <embedded name="type" class="TerminalType" use-column-prefix="false"/>
        <embedded name="principal" class="TerminalPrincipal" use-column-prefix="false"/>
        <embedded name="active" class="TerminalActive" use-column-prefix="false"/>
        <embedded name="assigmentDate" class="TerminalAssigmentDate" use-column-prefix="false"/>
