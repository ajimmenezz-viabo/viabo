<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\business\documents\domain\Document" table="t_business_commerces_documents">
        <embedded name="id" class="DocumentId" use-column-prefix="false"/>
        <field name="commerceId" type="commerce_id" column="CommerceId"/>
        <embedded name="name" class="DocumentName" use-column-prefix="false"/>
        <embedded name="storePath" class="DocumentStorePath" use-column-prefix="false"/>
        <embedded name="register" class="DocumentRegister" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>