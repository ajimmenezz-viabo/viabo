<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\management\terminalConsolidation\domain\TerminalConsolidation" table="t_management_terminal_conciliation">
        <embedded name="id" class="TerminalConsolidationId" use-column-prefix="false"/>
        <field name="commerceId" type="commerce_id" column="CommerceId"/>
        <embedded name="mainCardTransactionId" class="TerminalConsolidationMainCardTransactionId" use-column-prefix="false"/>
        <embedded name="transactionId" class="TerminalConsolidationTransactionId" use-column-prefix="false"/>
        <embedded name="amount" class="TerminalConsolidationAmount" use-column-prefix="false"/>
        <embedded name="terminalId" class="TerminalConsolidationTerminalId" use-column-prefix="false"/>
        <embedded name="userId" class="TerminalConsolidationUserId" use-column-prefix="false"/>
        <embedded name="registerDate" class="TerminalConsolidationRegisterDate" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>
